<%= form_for :truck, url: trucks_path do |f|%>
  <%= f.text_field :name, placeholder: "Truck Name" %><br>
  <%= f.text_field :latitude, placeholder: "Latitude" %><br>
  <%= f.text_field :longitude, placeholder: "Longitude" %><br>
  <%= f.text_field :url, placeholder: "Yelp Link" %><br>
  <%= f.submit "Create Truck" %>
<% end %>
